function filterUniqueProducts(products) {
    // Z채hle die H채ufigkeit jedes Produkts
    const productCounts = {};
    for (const product of products) {
        productCounts[product] = (productCounts[product] || 0) + 1;
    }

    // Filtere nur die Produkte, die nur einmal vorkommen
    const uniqueProducts = [];
    for (const product in productCounts) {
        if (productCounts[product] === 1) {
            uniqueProducts.push(product);
        }
    }

    return uniqueProducts;
}

// Beispielaufruf
const products = ["T-Shirt", "Hose", "T-Shirt", "M체tze"];
console.log(filterUniqueProducts(products)); // Ausgabe: ["Hose", "M체tze"]
